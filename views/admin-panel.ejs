<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>GV-Admin_panel</title>
  <link rel="stylesheet" type="text/css" href="/styles/controlpanel.css" />
  <script src="https://kit.fontawesome.com/e7764cfd68.js" crossorigin="anonymous"></script>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>

<body>
  <div class="admin-page-c">
    <!-- Option tray with button to open different functionality -->
    <div class="option-tray-c">
      <div class="gv-logo-c">
        <img src="/images/great-value-trademart.png" alt="great-value-trademart" />
      </div>
      <div class="option-tray-links-c">
        <div class="option-link-c">
          <p onclick="showSection('op-1')" class="option-link">Dashboard</p>
        </div>
        <div class="option-link-c">
          <p id="option-link-0" onclick="toggleOption(this.id)" class="option-link">Products
            <i id="up-arrow-0" class="fa-solid fa-angle-up up-arrow"></i>
            <i id="down-arrow-0" class="fa-solid fa-angle-down"></i>
          </p>
          <ul id="option-0" class="option hide">
            <li onclick="showSection('op-2')">Add product</li>
            <li onclick="showSection('op-3')">Edit product</li>
          </ul>
        </div>
        <div class="option-link-c">
          <p id="option-link-1" onclick="toggleOption(this.id)" class="option-link">Jobs
            <i id="up-arrow-1" class="fa-solid fa-angle-up up-arrow"></i>
            <i id="down-arrow-1" class="fa-solid fa-angle-down"></i>
          </p>
          <ul id="option-1" class="option hide">
            <li onclick="showSection('op-4')">Add job</li>
            <li onclick="showSection('op-5')">Edit job</li>
            <li onclick="showSection('op-6')">Job Applications</li>
          </ul>
        </div>
        <div class="option-link-c">
          <p id="option-link-2" onclick="toggleOption(this.id)" class="option-link">Offer
            <i id="up-arrow-2" class="fa-solid fa-angle-up up-arrow"></i>
            <i id="down-arrow-2" class="fa-solid fa-angle-down"></i>
          </p>
          <ul id="option-2" class="option hide">
            <li>Add offer</li>
            <li>Edit offer</li>
            <li>Delete offer</li>
          </ul>
        </div>
        <div class="option-link-c">
          <p id="option-link-3" onclick="toggleOption(this.id)" class="option-link">Orders
            <i id="up-arrow-3" class="fa-solid fa-angle-up up-arrow"></i>
            <i id="down-arrow-3" class="fa-solid fa-angle-down"></i>
          </p>
          <ul id="option-3" class="option hide">
            <li>Order recieved</li>
            <li>Pending order</li>
            <li>Order delivered</li>
          </ul>
        </div>
        <div class="option-link-c">
          <p class="option-link">Users</p>
        </div>
      </div>
    </div>
    <!-- Admin profile display bar HTML code -->
    <div class="admin-profile-c">
      <div class="search-bar-c">
        <form name="admin-search" action="/admin-search" method="post">
          <input id="admin-search-box" class="admin-search-input" type="search" placeholder="Search.."
            aria-label="Search through site content" name="search_input" required>
          <button class="admin-search-btn" type="submit"><i class="fa-solid fa-magnifying-glass"></i></button>
        </form>
      </div>
      <div class="admin-details-c">
        <div class="admin-name-display-c">
          <p class="admin-name">Nitin Shukla</p>
          <p class="user-type">Adminstrator (GreatValue)</p>
        </div>
        <form action="/logout" method="post">
          <button id="admin-Logout" class="admin-logout-btn"><i class="fa-solid fa-arrow-right-from-bracket"></i></button>
        </form>
        
      </div>
    </div>
    <!-- Notification bar HTML code -->
    <div class="notification-c">
      <div class="notification-card-c">
        <div class="notification-card-content-c">
          <p class="result-value">
            <%=products.length%>
          </p>
          <p class="value-tag">Total Prodcts</p>
        </div>
      </div>
      <div class="notification-card-c">
        <div class="notification-card-content-c">
          <p class="result-value">
            <%=valuation[0].total%>
          </p>
          <p class="value-tag">Products Valuation</p>
        </div>
      </div>
      <div class="notification-card-c">
        <div class="notification-card-content-c">
          <p class="result-value">18</p>
          <p class="value-tag">Delaer Enquiry</p>
        </div>
      </div>
      <div class="notification-card-c">
        <div class="notification-card-content-c">
          <p class="result-value">18</p>
          <p class="value-tag">Delaer Enquiry</p>
        </div>
      </div>
    </div>
    <!-- Admin control panel window HTML code -->
    <div id="admin-window" class="admin-editing-window-c">
      <!-- Admin dashboard HTML section id=1 -->
      <div id="op-1" class="dashboard-s">
        <div class="dashboard-heading-c">
          <p class="dashboard-heading">Dashboard</p>
        </div>
        <div class="card-holding-c">
          <div class="dashboard-card">
            <div class="card-heading-c">
              <p class="card-heading">Products</p>
            </div>
            <div class="report-c">
              <table class="report-table">
                <tr>
                  <td class="td-name">Total product</td>
                  <td class="td-value">
                    <%=products.length%>
                  </td>
                </tr>
                <tr>
                  <td class="td-name">Product category</td>
                  <td class="td-value">6</td>
                </tr>
                <tr>
                  <td class="td-name">Total product sold</td>
                  <td class="td-value">0</td>
                </tr>
                <tr>
                  <td class="td-name">Pending delivery</td>
                  <td class="td-value">0</td>
                </tr>
              </table>
            </div>
          </div>
          <div class="dashboard-card">
            <div class="card-heading-c">
              <p class="card-heading">Dealer Enquiry</p>
            </div>
            <div class="report-c">
              <table class="report-table">
                <tr>
                  <td class="td-name">Total Enquiry</td>
                  <td class="td-value">16</td>
                </tr>
                <tr>
                  <td class="td-name">Enquiry resolved</td>
                  <td class="td-value">0</td>
                </tr>
                <tr>
                  <td class="td-name">Enquiry pending</td>
                  <td class="td-value">1</td>
                </tr>
              </table>
            </div>
          </div>
        </div>
      </div>
      <!-- Add product section html code section id=2 -->
      <div id="op-2" class="option-c">
        <div class="option-heading-c">
          <p class="option-heading">Add new product</p>
        </div>
        <form id="add-product-form" class="add-product-form">
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productName">Product name : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productName" name="p_name"
                placeholder="Product name" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productQuantity">Product Quantity : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productQuantity" name="p_quantity"
                placeholder="Product quantity" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productRating">Product rating : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productRating" name="p_rating"
                placeholder="Product rating" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productSp">Product selling price : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productSp" name="p_sp"
                placeholder="Product selling price" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productMrp">Product MRP : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productMrp" name="p_mrp" placeholder="Product MRP"
                autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productFeatureOne">Product feature : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productFeatureOne" name="p_feature_1"
                placeholder="Product feature 1" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productFeatureTwo" name="p_feature_2"
                placeholder="Product feature 2" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productFeatureThree" name="p_feature_3"
                placeholder="Product feature 3" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productFeatureFour" name="p_feature_4"
                placeholder="Product feature 4" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productFeatureFive" name="p_feature_5"
                placeholder="Product feature 5" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productColor">Product Colour : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productColor" name="p_color"
                placeholder="Product Colour" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productMaterial">Product material : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productMaterial" name="p_material"
                placeholder="Product material, eg metal, plastic etc.." autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productLength">Product dimension : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productLength" name="p_length"
                placeholder="Product length in cm" autocomplete="off" required />
              <input class="add-product-form-input" type="number" id="productWidth" name="p_width"
                placeholder="Product width in cm" autocomplete="off" required />
              <input class="add-product-form-input" type="number" id="productHeight" name="p_height"
                placeholder="Product height in cm" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productWeightLimit">Product weight limit : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productWeightLimit" name="p_weight_limit"
                placeholder="Max weight product can hold in Kg.." autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productSuitable1">Product suitable for : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productSuitable1" name="p_suitable"
                placeholder="Product suitable for, eg Office, living room etc.." autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productSuitable2" name="p_suitable"
                placeholder="Product suitable for, eg Office, living room etc.." autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productSuitable3" name="p_suitable"
                placeholder="Product suitable for, eg Office, living room etc.." autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productSuitable4" name="p_suitable"
                placeholder="Product suitable for, eg Office, living room etc.." autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productWheel">Total wheels : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productWheel" name="p_wheel"
                placeholder="Total wheel" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productWeight">Product weight : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="number" id="productWeight" name="p_weight"
                placeholder="Product weight in grams" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="item1">Category : </label>
            <div class="multiple-input-c">
              <fieldset class="checkbox">
                <div class="checkbox-c">
                  <input class="checkbox" type="checkbox" id="item1" name="item1" value="chair" />
                  <label for="item1">Chair</label>
                </div>
                <div class="checkbox-c">
                  <input class="checkbox" type="checkbox" id="item2" name="item2" value="Table" />
                  <label for="item2">Table</label>
                </div>
                <div class="checkbox-c">
                  <input class="checkbox" type="checkbox" id="item3" name="item3" value="Storage unit" />
                  <label for="item3">Storage unit</label>
                </div>
                <div class="checkbox-c">
                  <input class="checkbox" type="checkbox" id="item4" name="item4" value="Sofa" />
                  <label for="item4">Sofa</label>
                </div>
                <div class="checkbox-c">
                  <input class="checkbox" type="checkbox" id="item5" name="item5" value="Safe and locker" />
                  <label for="item5">Safe and locker</label>
                </div>
                <div class="checkbox-c">
                  <input class="checkbox" type="checkbox" id="item6" name="item6" value="Currency handling product" />
                  <label for="item6">Currency handling product</label>
                </div>
              </fieldset>
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productAssembly">Assembly required : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productAssembly" name="p_assembly"
                placeholder="Assembly required" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productBrand">Product brand : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productBrand" name="p_brand"
                placeholder="Product brand" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productModel">Product model number : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productModel" name="p_model"
                placeholder="Product model number" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productWarranty">Warranty : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productWarranty" name="p_warranty"
                placeholder="Warranty" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="inBox">In the box : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="inBox" name="in_box" placeholder="In the box"
                autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="productDescriptioOne">Product description : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="productDescriptioOne" name="p_description1"
                placeholder="Product description 1" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioTwo" name="p_description2"
                placeholder="Product description 2" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioThree" name="p_description3"
                placeholder="Product description 3" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioFour" name="p_description4"
                placeholder="Product description 4" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioFive" name="p_description5"
                placeholder="Product description 5" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioSix" name="p_description6"
                placeholder="Product description 6" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioSeven" name="p_description7"
                placeholder="Product description 7" autocomplete="off" required />
              <input class="add-product-form-input" type="text" id="productDescriptioEight" name="p_description8"
                placeholder="Product description 8" autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="power">Power required : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="text" id="power" name="power" placeholder="Power required?"
                autocomplete="off" required />
            </div>
          </div>
          <div class="add-product-form-input-c">
            <label class="add-product-form-input-label" for="imageOne">Upload images : </label>
            <div class="multiple-input-c">
              <input class="add-product-form-input" type="file" id="imageOne" name="image1" autocomplete="off"
                required />
              <input class="add-product-form-input" type="file" id="imageTwo" name="image2" autocomplete="off"
                required />
              <input class="add-product-form-input" type="file" id="imageThree" name="image3" autocomplete="off"
                required />
              <input class="add-product-form-input" type="file" id="imageFour" name="image4" autocomplete="off"
                required />
              <input class="add-product-form-input" type="file" id="imageFive" name="image5" autocomplete="off"
                required />
              <input class="add-product-form-input" type="file" id="imageSix" name="image6" autocomplete="off"
                required />
            </div>
          </div>
          <div class="add-product-form-submit-btn-c">
            <button class="add-product-form-submit-btn">Submit</button>
          </div>
        </form>
      </div>
      <!-- Edit product section html code section id=3 -->
      <div id="op-3" class="option-c">
        <div class="option-heading-c">
          <p class="option-heading">Edit or delete product</p>
        </div>
        <div class="list-items-c">
          <div class="list-search-c">
            <div class="list-search-box">
              <input id="product-list-search" class="list-search" type="search" placeholder="Search.."
                aria-label="Search through site content" name="search_input" required>
              <button class="list-search-btn" type="submit"><i class="fa-solid fa-magnifying-glass"></i></button>
            </div>
          </div>
          <div class="product-list-items-c">
            <table class="list-table">
              <tr class="list-tr">
                <th class="list-th list-a">S no.</th>
                <th class="list-th list-b">Product name</th>
                <th class="list-th list-c">Brand</th>
                <th class="list-th list-d">Model number</th>
                <th class="list-th list-e">Category</th>
                <th class="list-th list-edit">Edit</th>
                <th class="list-th list-delete">Delete</th>
              </tr>
              <% for(let i=0; i<products.length;i++) {%>
                <tr class="list-tr">
                  <td class="list-td list-a">
                    <%=products[i].id%>
                  </td>
                  <td class="list-td list-b">
                    <%=products[i].p_name%>
                  </td>
                  <td class="list-td list-c">
                    <%=products[i].brand%>
                  </td>
                  <td class="list-td list-d">
                    <%=products[i].model_number%>
                  </td>
                  <td class="list-td list-e">
                    <%=products[i].category%>
                  </td>
                  <td id="ep-<%=products[i].id%>" class="list-td list-edit" onclick="editProduct(this.id)"><i
                      class="fa-solid fa-pen-to-square" style="color: #FFD43B;"></i>
                  </td>
                  <td id="dp-<%=products[i].id%>" class="list-td list-delete" onclick="deleteProduct(this.id)"><i
                      class="fa-solid fa-trash" style="color: #ec2213;"></i></td>
                </tr>
                <%}%>
            </table>
          </div>
        </div>
        <div class="product-edit-form-c"></div>
      </div>
      <!-- ADD job section html code section id=4 -->
      <div id="op-4" class="option-c">
        <div class="option-heading-c">
          <p class="option-heading">Add new job</p>
        </div>
        <div id="add-job-form" class="add-job-form">
          <div class="add-job-form-input-c">
            <label class="add-job-form-input-label" for="jobTitle">Job Title : </label>
            <input class="add-job-form-input" type="text" id="jobTitle" name="job_title" placeholder="Job title"
              autocomplete="off" required />
          </div>
          <div class="add-job-form-input-c">
            <label class="add-job-form-input-label" for="experience">Experience : </label>
            <input class="add-job-form-input" type="text" id="experience" name="experience" placeholder="Experience"
              autocomplete="off" required />
          </div>
          <div class="add-job-form-input-c">
            <label class="add-job-form-input-label" for="jobLocation">Job location : </label>
            <input class="add-job-form-input" type="text" id="jobLocation" name="job_location"
              placeholder="Job location" autocomplete="off" required />
          </div>
          <div class="add-job-form-input-c">
            <label class="add-job-form-input-label" for="salary">Salary : </label>
            <input class="add-job-form-input" type="text" id="salary" name="salary" placeholder="Salary"
              autocomplete="off" required />
          </div>
          <div class="add-job-form-input-c">
            <label class="add-job-form-input-label" for="description">Description : </label>
            <textarea class="add-job-form-input" type="text" id="description" name="description"
              placeholder="Job Description" autocomplete="off" required></textarea>
          </div>
          <div class="add-job-form-submit-btn-c">
            <button class="add-job-form-submit-btn" type="submit" onclick="addJob()">Submit</button>
          </div>
        </div>
      </div>
      <!-- Edit job section html code section id=5 -->
      <div id="op-5" class="option-c">
        <div class="option-heading-c">
          <p class="option-heading">Edit or delete product</p>
        </div>
        <div class="list-items-c">
          <div class="list-search-c">
            <div class="list-search-box">
              <input id="job-list-search" class="list-search" type="search" placeholder="Search.."
                aria-label="Search through site content" name="search_input" required>
              <button class="list-search-btn" type="submit"><i class="fa-solid fa-magnifying-glass"></i></button>
            </div>
          </div>
          <div class="list-table-c">
            <table class="list-table">
              <tr class="list-tr">
                <th class="list-th list-a">Job id.</th>
                <th class="list-th list-b">Job title</th>
                <th class="list-th list-c">Experience</th>
                <th class="list-th list-d">Location</th>
                <th class="list-th list-e">Salary</th>
                <th class="list-th list-edit">Edit</th>
                <th class="list-th list-delete">Delete</th>
              </tr>
              <% for(let i=0;i<jobDetails.length;i++) {%>
                <tr class="list-tr">
                  <td class="list-td list-a">
                    <%=jobDetails[i].id%>
                  </td>
                  <td class="list-td list-b">
                    <%=jobDetails[i].title%>
                  </td>
                  <td class="list-td list-c">
                    <%=jobDetails[i].experience%>
                  </td>
                  <td class="list-td list-d">
                    <%=jobDetails[i].location%>
                  </td>
                  <td class="list-td list-e">
                    <%=jobDetails[i].salary%>
                  </td>
                  <td id="ej-<%=jobDetails[i].id%>" class="list-td list-edit" onclick="editJob(this.id)"><i
                      class="fa-solid fa-pen-to-square" style="color: #FFD43B;"></i>
                  </td>
                  <td id="dj-<%=jobDetails[i].id%>" class="list-td list-delete" onclick="deleteJob(this.id)"><i
                      class="fa-solid fa-trash" style="color: #ec2213;"></i></td>
                </tr>
                <%}%>
            </table>
          </div>
        </div>
      </div>
      <!-- Job application status html code section id=5 -->
      <div id="op-6" class="option-c">
        <div class="option-heading-c">
          <p class="option-heading">Job applications</p>
        </div>
        <div class="list-items-c">
          <div class="list-search-c">
            <div class="list-search-box">
              <input id="job-list-search" class="list-search" type="search" placeholder="Search.."
                aria-label="Search through site content" name="search_input" required>
              <button class="list-search-btn" type="submit"><i class="fa-solid fa-magnifying-glass"></i></button>
            </div>
          </div>
          <div class="list-table-c">
            <table class="list-table">
              <tr class="list-tr">
                <th class="list-th list-i">Post</th>
                <th class="list-th list-ii">Name</th>
                <th class="list-th list-iii">Email</th>
                <th class="list-th list-iv">Phone Number</th>
                <th class="list-th list-v">Qualification</th>
                <th class="list-th list-vi">Degree</th>
                <th class="list-th list-vii">Resume</th>
              </tr>
              <% for(let i=0;i<jonApplication.length;i++) {%>
                <tr class="list-tr">
                  <td class="list-td list-i">
                    <%=jonApplication[i].applied_for%>
                  </td>
                  <td class="list-td list-ii">
                    <%=jonApplication[i].name%>
                  </td>
                  <td class="list-td list-iii">
                    <%=jonApplication[i].email%>
                  </td>
                  <td class="list-td list-iv">
                    <%=jonApplication[i].phone%>
                  </td>
                  <td class="list-td list-v">
                    <%=jonApplication[i].qualification%>
                  </td>
                  <td class="list-td list-vi" onclick="editJob(this.id)">
                      <%=jonApplication[i].degree%>
                  </td>
                  <td class="list-td list-vii" onclick="deleteJob(this.id)">
                    <a href="<%=jonApplication[i].cv_url%>" target="_blank"><i class="fa-solid fa-download"></i></a>
                    </td>
                </tr>
                <%}%>
            </table>
          </div>
        </div>
      </div>
    </div>

